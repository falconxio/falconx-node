{
    "extends": ["airbnb"],
    "env": {
      "node": true
    },
    "rules": {
      "max-len":  ["error", 150, 2, {
        "ignoreUrls": true,
        "ignoreComments": false,
        "ignoreRegExpLiterals": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true
      }],
      "import/no-cycle": 0
    }
  }
  